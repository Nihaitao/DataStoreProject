<sqlMap namespace="CollectionMapper" xmlns="http://ibatis.apache.org/mapping"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <statements>
    <select id="selectCollection" resultClass="map" parameterClass="map">
      select * from w_collection where BusType=#BusType# and StuID=#StuID#
    </select>

  </statements>
</sqlMap>