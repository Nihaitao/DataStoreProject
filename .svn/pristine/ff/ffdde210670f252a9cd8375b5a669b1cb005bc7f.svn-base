        /// <summary>
        /// 章节名称
        /// </summary>
        public string Name { set; get; }


        /// <summary>
        /// 父节点
        /// </summary>
        public int CID { set; get; }


        /// <summary>
        /// 网课主表id
        /// </summary>
        public int Course_ID { set; get; }


        /// <summary>
